{
    "name": "forum_check",
    "version": "1.0",
    "description": "Build an Extension!",
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "permissions": ["storage", "declarativeContent", "activeTab", "https://pitstop.manageengine.com/"],
    "page_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/forum_test16.png",
            "32": "images/forum_test32.png",
            "48": "images/forum_test48.png",
            "128": "images/forum_test128.png"
        }
    },
    "icons": {
        "16": "images/forum_test16.png",
        "32": "images/forum_test32.png",
        "48": "images/forum_test48.png",
        "128": "images/forum_test128.png"
    },
    "manifest_version": 2
  }